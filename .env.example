# Environment variables for dbt PostgreSQL connection
# Copy this file to .env and fill in your actual values

# Development Environment
DBT_POSTGRES_USER=your_postgres_username
DBT_POSTGRES_PASSWORD=your_postgres_password
DBT_POSTGRES_DB=your_database_name

# Production Environment (optional)
DBT_POSTGRES_HOST_PROD=your_prod_host
DBT_POSTGRES_USER_PROD=your_prod_username
DBT_POSTGRES_PASSWORD_PROD=your_prod_password
DBT_POSTGRES_DB_PROD=your_prod_database_name

# For Tableau integration
DBT_SEMANTIC_LAYER_URL=https://semantic-layer.getdbt.com/api/graphql
DBT_SEMANTIC_LAYER_TOKEN=your_dbt_cloud_token